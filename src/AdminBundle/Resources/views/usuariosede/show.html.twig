    {% extends 'admin_base.html.twig' %}

    {% set __title = 'Usuario Sede' %}

    {% block content %}
    <div class="box box-success">
        <div class="box-body">
            <div class="row">
                <div class="col-md-9">
                    <table class="table">
                        <tbody>
                            <tr>
                                <th>Id</th>
                                    <td>{{ usuarioSede.id }}</td>
                            </tr>
                            <tr>
                                <th>Sede</th>
                                <td>{{ usuarioSede.sede }}</td>
                            </tr>
                            <tr>
                                <th>Usuario</th>
                                <td>{{ usuarioSede.user }}</td>
                            </tr>
                            <tr>
                                <th>Activo</th>
                                    <td>{% if usuarioSede.activo %}<span class="label label-success">Si</span{% else %}<span class="label label-danger">No</span>{% endif %}</td>
                            </tr>
                            <tr>
                                <th>Fecha Creación</th>
                                    <td>{% if usuarioSede.fechaCreacion %}{{ usuarioSede.fechaCreacion|date('d-m-Y H:i:s') }}{% endif %}</td>
                            </tr>
                            <tr>
                                <th>Fecha Actualización</th>
                                    <td>{% if usuarioSede.fechaActualizacion %}{{ usuarioSede.fechaActualizacion|date('d-m-Y H:i:s') }}{% endif %}</td>
                            </tr>
                        </tbody>
                    </table>

                    <div class="box-footer">
                        <a class="btn btn-success" href="{{ path('usuariosede_edit', { 'id': usuarioSede.id }) }}"> Editar</a>
                        <a class="btn btn-default" href="{{ path('usuariosede_index') }}"> Volver al Listado</a>
                    </div>

                </div>
            </div>
        </div>
    </div>

    {% endblock %}
