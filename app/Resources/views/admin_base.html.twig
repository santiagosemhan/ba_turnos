<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}{% if __title is defined %} {{ __title }} - {% endif %}Panel de Control{% endblock %}</title>

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/AdminLTE/bootstrap/css/bootstrap.min.css') }}">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/AdminLTE/bootstrap/css/font-awesome.min.css') }}">
    <!-- Ionicons -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/AdminLTE/bootstrap/css/ionicons.min.css') }}">
    <!-- jvectormap -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css') }}">
    <!-- Theme style -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/AdminLTE/dist/css/AdminLTE.min.css') }}">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="{{ asset('assets/vendor/AdminLTE/dist/css/skins/_all-skins.min.css') }}">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="{{ asset('assets/vendor/AdminLTE/bootstrap/js/html5shiv.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/AdminLTE/bootstrap/js/respond.min.js') }}"></script>
    <![endif]-->
    {% block stylesheets %}{% endblock %}

</head>


<body class="hold-transition skin-green sidebar-mini">
<!-- Site wrapper -->
<div class="wrapper">

    <!-- Main Header -->
    <header class="main-header">

        <!-- Logo -->
        <a href="{{ path('admin_homepage') }}" class="logo">
            <!-- mini logo for sidebar mini 50x50 pixels -->
            <span class="logo-mini"><b>BA</b>T</span>
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"><b>BA Turnos</b></span>
        </a>

        <!-- Header Navbar -->
        <nav class="navbar navbar-static-top">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>

            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <span class="hidden-xs">{{ app.user }}</span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                <p>
                                    {{ app.user }}
                                </p>
                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a href="{{ path('cambiar-password') }}" class="btn btn-default btn-flat">Cambiar Contraseña</a>
                                </div>
                                <div class="pull-right">
                                    <a href="{{ path('fos_user_security_logout') }}" class="btn btn-default btn-flat">Cerrar Sesión</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">

        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">

            <ul class="sidebar-menu">
                <li class="header">Menu</li>
                <li class="active treeview">
                    <ul class="treeview-menu">
                        <li class="active"><a href="{{ path('admin_homepage') }}"><i class="fa fa-circle-o"></i> Inicio</a></li>
                    </ul>
                </li>
                <li class="treeview">
                    <a href="{{ path('turno_administrar') }}">
                        <i class="fa fa-files-o"></i>
                        <span>Administración Turnos</span>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="fa fa-th"></i> <span>Administrar Box</span>
                    </a>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-pencil-square"></i>
                        <span>Parametrización</span>
                        <span class="pull-right-container">
                          <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="{{ path('sede_index') }}"><i class="fa  fa-building-o"></i> Sede</a></li>
                        <li><a href=""><i class="fa  fa-inbox"></i> Box</a></li>
                        <li><a href="{{ path('tipotramite_index') }}"><i class="fa fa-file-pdf-o"></i> Tipo Tramite</a></li>
                        <li><a href="{{ path('feriado_index') }}"><i class="fa fa-sign-out"></i> Feriado</a></li>
                        <li><a href="{{ path('turnossede_index') }}"><i class="fa fa-calendar-check-o"></i> Turnos por Sede</a></li>
                        <li><a href="{{ path('texto_mail_index') }}"><i class="fa fa-envelope-o"></i> Parametrización Mail</a></li>

                    </ul>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-user"></i>
                        <span>Usuarios</span>
                        <span class="pull-right-container">
                          <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="{{ path('usuariosede_index') }}"><i class="fa  fa-building-o"></i> Sede Usuario</a></li>
                    </ul>
                </li>

            </ul>



            <!-- /.sidebar-menu -->
        </section>
        <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            {% if __title is defined %}
                <h1 class="page-header">
                    {{ __title }} {% if __descripcion_opcional is defined %}<small>{{ __descripcion_opcional | raw }}</small>{%  endif %}
                </h1>
            {% endif %}
            {#<ol class="breadcrumb">#}
            {#<li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>#}
            {#<li class="active">Here</li>#}
            {#</ol>#}
        </section>

        <!-- Main content -->
        <section class="content">
            {%  include 'flash_messages.html.twig' %}
            {% block content %}{% endblock %}

        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->

    {%  include 'modals.html.twig' %}

    <!-- Main Footer -->
    <footer class="main-footer">
        <!-- To the right -->
        <div class="pull-right hidden-xs">
            <!--<a href="#">-->
                <img height="34px" src="{{ asset('assets/vendor/images/logo-bap.png') }}"/>
            <!-- </a>-->
         </div>
             <!-- Default to the left -->
        <strong>Copyright &copy; {{ 'now' | date('Y') }}</strong> Todos los derechos reservados.
    </footer>

</div><!-- ./wrapper -->

{% block footer_javascript %}

    <!-- jQuery 2.2.3 -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/jQuery/jquery-2.2.3.min.js') }}"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="{{ asset('assets/vendor/AdminLTE/bootstrap/js/jquery-ui.min.js') }}"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.6 -->
    <script src="{{ asset('assets/vendor/AdminLTE/bootstrap/js/bootstrap.min.js') }}"></script>
    <!-- Morris.js charts -->
    <script src="{{ asset('assets/vendor/AdminLTE/bootstrap/js/raphael-min.js') }}"></script>
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/morris/morris.min.js') }}"></script>
    <!-- Sparkline -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
    <!-- jvectormap -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
    <!-- jQuery Knob Chart -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/knob/jquery.knob.js') }}"></script>
    <!-- daterangepicker -->
    <script src="{{ asset('assets/vendor/AdminLTE/bootstrap/js/moment.min.js') }}"></script>
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/daterangepicker/daterangepicker.js') }}"></script>
    <!-- datepicker -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/datepicker/bootstrap-datepicker.js') }}"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>
    <!-- Slimscroll -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
    <!-- FastClick -->
    <script src="{{ asset('assets/vendor/AdminLTE/plugins/fastclick/fastclick.js') }}"></script>
    <!-- AdminLTE App -->
    <script src="{{ asset('assets/vendor/AdminLTE/dist/js/app.min.js') }}"></script>

{% endblock %}


</body>
</html>
